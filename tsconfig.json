{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2019",
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@types",
    ],
    "strictPropertyInitialization": false,
  },
  "include": [
    "src",
  ],
}